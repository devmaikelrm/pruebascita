{
  "name": "spanishconsular-worker",
  "version": "1.0.0",
  "description": "Playwright worker for Spanish consular appointment automation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p ./tsconfig.json",
    "start": "node dist/index.js",
    "postinstall": "npx playwright install chromium",
    "discover": "tsx src/discover.ts",
    "prebuild": "rimraf dist"
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "playwright-chromium": "^1.40.0",
    "dotenv": "^16.0.0",
    "sharp": "^0.32.0",
    "node-cron": "^3.0.0",
    "pg": "^8.11.0",
    "@neondatabase/serverless": "^0.9.0",
    "drizzle-orm": "^0.33.0",
    "ws": "^8.14.2",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "drizzle-zod": "^0.7.0",
    "zod": "^3.24.2",
    "@repo/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "rimraf": "^6.0.1"
  }
}
